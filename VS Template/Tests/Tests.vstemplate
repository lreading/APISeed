<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>APISeed Tests Project</Name>
    <Description>API Seed: Tests project with standard CRUD tests</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>APISeed.Tests</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.png</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="APISeed.Tests.csproj" File="APISeed.Tests.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="app.config">app.config</ProjectItem>
      <Folder Name="Errors" TargetFolderName="Errors">
        <ProjectItem ReplaceParameters="true" TargetFileName="Error1.xml">Error1.xml</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ErrorLogTests.cs">ErrorLogTests.cs</ProjectItem>
      </Folder>
      <Folder Name="Integration" TargetFolderName="Integration">
        <ProjectItem ReplaceParameters="true" TargetFileName="BaseTest.cs">BaseTest.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="CRUDTests.cs">CRUDTests.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ErrorCRUDTest.cs">ErrorCRUDTest.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="SetupTearDown.cs">SetupTearDown.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="TestConnectionManager.cs">TestConnectionManager.cs</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="packages.config">packages.config</ProjectItem>
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
</VSTemplate>